<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns:pattern="http://www.mulesoft.org/schema/mule/pattern"
	xmlns:ws="http://www.mulesoft.org/schema/mule/ws" xmlns:mulexml="http://www.mulesoft.org/schema/mule/xml"
	xmlns:cxf="http://www.mulesoft.org/schema/mule/cxf" xmlns:http="http://www.mulesoft.org/schema/mule/http"
	xmlns:servlet="http://www.mulesoft.org/schema/mule/servlet" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:spring="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.mulesoft.org/schema/mule/pattern http://www.mulesoft.org/schema/mule/pattern/current/mule-pattern.xsd
http://www.mulesoft.org/schema/mule/xml http://www.mulesoft.org/schema/mule/xml/current/mule-xml.xsd
http://www.mulesoft.org/schema/mule/cxf http://www.mulesoft.org/schema/mule/cxf/current/mule-cxf.xsd
http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd
http://www.mulesoft.org/schema/mule/servlet http://www.mulesoft.org/schema/mule/servlet/current/mule-servlet.xsd
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-current.xsd
http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/ws http://www.mulesoft.org/schema/mule/ws/current/mule-ws.xsd">

	<flow name="WS-Password-CodeFirst-HTTP-Listener">
		<http:listener config-ref="Listener-Application"
			doc:name="HTTP Inbound Endpoint" path="${endpoint.password.codeFirst.path}" />
		<flow-ref name="WS-Password-CodeFirst-Endpoint"
			doc:name="Send to endpoint" />
	</flow>
	<sub-flow name="WS-Password-CodeFirst-Endpoint">
		<logger message="Begins the password protected Code-First endpoint flow"
			level="INFO" doc:name="Logger" />
		<cxf:jaxws-service validationEnabled="${endpoint.validation}"
			doc:name="Code-First endpoint interface" serviceClass="${endpoint.unsecure.codeFirst.interface.class}"
			namespace="${wsdl.namespace}">
			<cxf:ws-security>
				<cxf:ws-config>
					<cxf:property key="action" value="UsernameToken" />
					<cxf:property key="passwordCallbackRef" value-ref="passwordCallbackHandler" />
				</cxf:ws-config>
			</cxf:ws-security>
		</cxf:jaxws-service>
		<component doc:name="Code-First endpoint implementation">
			<spring-object bean="codeFirstUnsecureEndpoint" />
		</component>
		<logger message="Ends the password protected Code-First endpoint flow"
			level="INFO" doc:name="Logger" />
	</sub-flow>

	<flow name="WS-Password-WSDLFirst-HTTP-Listener">
		<http:listener config-ref="Listener-Application"
			doc:name="HTTP Inbound Endpoint" path="${endpoint.password.wsdlFirst.path}" />
		<flow-ref name="WS-Password-WSDLFirst-Endpoint"
			doc:name="Send to endpoint" />
	</flow>
	<sub-flow name="WS-Password-WSDLFirst-Endpoint">
		<logger message="Begins the password protected WSDL-First endpoint flow"
			level="INFO" doc:name="Logger" />
		<cxf:jaxws-service validationEnabled="${endpoint.validation}"
			doc:name="WSDL-First endpoint interface" serviceClass="${endpoint.unsecure.wsdlFirst.interface.class}"
			namespace="${wsdl.namespace}" wsdlLocation="${wsdl.path}" service="${wsdl.service}">
			<cxf:ws-security>
				<cxf:ws-config>
					<cxf:property key="action" value="UsernameToken" />
					<cxf:property key="passwordCallbackRef" value-ref="passwordCallbackHandler" />
				</cxf:ws-config>
			</cxf:ws-security>
		</cxf:jaxws-service>
		<component doc:name="WSDL-First endpoint implementation">
			<spring-object bean="wsdlFirstUnsecureEndpoint" />
		</component>
		<logger message="Ends the password protected WSDL-First endpoint flow"
			level="INFO" doc:name="Logger" />
	</sub-flow>

	<flow name="WS-Password-Simple-HTTP-Listener">
		<http:listener config-ref="Listener-Application"
			path="${endpoint.password.simple.path}" doc:name="HTTP Inbound Endpoint" />
		<flow-ref name="WS-Password-Simple-Endpoint" doc:name="Send to endpoint" />
	</flow>
	<sub-flow name="WS-Password-Simple-Endpoint">
		<logger message="Begins the password protected simple endpoint flow"
			level="INFO" doc:name="Logger" />
		<cxf:simple-service serviceClass="${endpoint.unsecure.simple.class}"
			service="${wsdl.service}" doc:name="Simple endpoint">
			<cxf:ws-security>
				<cxf:ws-config>
					<cxf:property key="action" value="UsernameToken" />
					<cxf:property key="passwordCallbackRef" value-ref="passwordCallbackHandler" />
				</cxf:ws-config>
			</cxf:ws-security>
		</cxf:simple-service>
		<component doc:name="Simple endpoint implementation">
			<spring-object bean="simpleUnsecureEndpoint" />
		</component>
		<logger message="Ends the password protected simple endpoint flow"
			level="INFO" doc:name="Logger" />
	</sub-flow>

	<flow name="WS-Password-Proxy-HTTP-Listener">
		<http:listener config-ref="Listener-Application"
			doc:name="HTTP Inbound Endpoint" path="${endpoint.password.proxy.path}" />
		<flow-ref name="WS-Password-Proxy-Endpoint" doc:name="Send to endpoint" />
	</flow>
	<sub-flow name="WS-Password-Proxy-Endpoint">
		<logger message="Begins the password protected proxy endpoint flow"
			level="INFO" doc:name="Logger" />
		<cxf:proxy-service namespace="${wsdl.namespace}"
			payload="envelope" port="${wsdl.port}" service="${wsdl.service}"
			doc:name="Proxy endpoint" wsdlLocation="${wsdl.path}"
			enableMuleSoapHeaders="false">
			<cxf:ws-security>
				<cxf:ws-config>
					<cxf:property key="action" value="UsernameToken" />
					<cxf:property key="passwordCallbackRef" value-ref="passwordCallbackHandler" />
				</cxf:ws-config>
			</cxf:ws-security>
		</cxf:proxy-service>
		<flow-ref doc:name="Copy HTTP Headers" name="Copy-Headers" />
		<cxf:proxy-client doc:name="Proxy client"
			enableMuleSoapHeaders="false" payload="envelope" />
		<outbound-endpoint doc:name="Unsecure code-first endpoint"
			ref="Endpoint-Unsecure-CodeFirst" exchange-pattern="request-response" />
		<flow-ref doc:name="Copy HTTP Headers" name="Copy-Headers" />
		<logger message="Ends the password protected proxy endpoint flow"
			level="INFO" doc:name="Logger" />
	</sub-flow>

	<flow name="WS-Password-Consumer-HTTP-Listener">
		<http:listener config-ref="Listener-Application"
			doc:name="HTTP Inbound Endpoint" path="${endpoint.password.consumer.path}" />
		<flow-ref name="WS-Password-Consumer-Endpoint"
			doc:name="Send to endpoint" />
	</flow>
	<sub-flow name="WS-Password-Consumer-Endpoint">
		<logger message="Begins the password protected consumer endpoint flow"
			level="INFO" doc:name="Logger" />
		<flow-ref name="Transform-ConsumerRequest" doc:name="Transform to a valid request" />
		<ws:consumer config-ref="Consumer-Password-CodeFirst"
			operation="${wsdl.operation}" doc:name="Password protected code-first consumer" />
		<mulexml:dom-to-xml-transformer
			doc:name="Object to XML" />
		<logger message="Ends the password protected consumer endpoint flow"
			level="INFO" doc:name="Logger" />
	</sub-flow>

</mule>
