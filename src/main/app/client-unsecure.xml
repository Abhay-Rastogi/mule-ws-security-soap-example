<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns:jms="http://www.mulesoft.org/schema/mule/jms" xmlns:pattern="http://www.mulesoft.org/schema/mule/pattern"
   xmlns:ws="http://www.mulesoft.org/schema/mule/ws" xmlns:mulexml="http://www.mulesoft.org/schema/mule/xml"
   xmlns:cxf="http://www.mulesoft.org/schema/mule/cxf" xmlns:http="http://www.mulesoft.org/schema/mule/http"
   xmlns:servlet="http://www.mulesoft.org/schema/mule/servlet" xmlns="http://www.mulesoft.org/schema/mule/core"
   xmlns:doc="http://www.mulesoft.org/schema/mule/documentation" xmlns:spring="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.mulesoft.org/schema/mule/jms http://www.mulesoft.org/schema/mule/jms/current/mule-jms.xsd
http://www.mulesoft.org/schema/mule/pattern http://www.mulesoft.org/schema/mule/pattern/current/mule-pattern.xsd
http://www.mulesoft.org/schema/mule/xml http://www.mulesoft.org/schema/mule/xml/current/mule-xml.xsd
http://www.mulesoft.org/schema/mule/cxf http://www.mulesoft.org/schema/mule/cxf/current/mule-cxf.xsd
http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd
http://www.mulesoft.org/schema/mule/servlet http://www.mulesoft.org/schema/mule/servlet/current/mule-servlet.xsd
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-current.xsd
http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/ws http://www.mulesoft.org/schema/mule/ws/current/mule-ws.xsd">

   <flow name="Client-Unsecure-CodeFirst-HTTP-Listener">
      <http:listener config-ref="Listener-Application"
         path="${client.unsecure.codeFirst.path}" doc:name="HTTP Inbound Endpoint" />
      <flow-ref name="Read-HTTP-Variables" doc:name="Read HTPP variables" />
      <flow-ref name="Client-Unsecure-CodeFirst-Endpoint" doc:name="Send request" />
      <response>
         <flow-ref name="Transform-Response" doc:name="Transform to a valid response" />
      </response>
   </flow>
   <sub-flow name="Client-Unsecure-CodeFirst-Endpoint">
      <logger message="Begins the unsecure Code-First client flow" level="INFO"
         doc:name="Logger" />
      <cxf:jaxws-client serviceClass="${endpoint.codeFirst.interface}"
         doc:name="Code-First client" operation="${wsdl.operation}" />
      <outbound-endpoint doc:name="Unsecure Code-First endpoint"
         ref="Endpoint-Unsecure-CodeFirst" />
      <logger message="Ends the unsecure Code-First client flow" level="INFO"
         doc:name="Logger" />
   </sub-flow>

   <flow name="Client-Unsecure-WSDLFirst-HTTP-Listener">
      <http:listener config-ref="Listener-Application"
         path="${client.unsecure.wsdlFirst.path}" doc:name="HTTP Inbound Endpoint" />
      <flow-ref name="Read-HTTP-Variables" doc:name="Read HTPP variables" />
      <flow-ref name="Client-Unsecure-WSDLFirst-Endpoint" doc:name="Send request" />
      <response>
         <flow-ref name="Transform-Response" doc:name="Transform to a valid response" />
      </response>
   </flow>
   <sub-flow name="Client-Unsecure-WSDLFirst-Endpoint">
      <logger message="Begins the unsecure WSDL-First client flow" level="INFO"
         doc:name="Logger" />
      <cxf:jaxws-client clientClass="${client.wsdlFirst.class}"
         wsdlLocation="${wsdl.path}" port="${wsdl.port}" doc:name="WSDL-First client"
         operation="${wsdl.operation}" />
      <outbound-endpoint doc:name="Unsecure WSDL-First endpoint"
         ref="Endpoint-Unsecure-WSDLFirst" />
      <logger message="Ends the unsecure WSDL-First client flow" level="INFO"
         doc:name="Logger" />
   </sub-flow>

   <flow name="Client-Unsecure-Simple-HTTP-Listener">
      <http:listener config-ref="Listener-Application" path="${client.unsecure.simple.path}"
         doc:name="HTTP Inbound Endpoint" />
      <flow-ref name="Read-HTTP-Variables" doc:name="Read HTPP variables" />
      <flow-ref name="Client-Unsecure-Simple-Endpoint" doc:name="Send request" />
      <response>
         <flow-ref name="Transform-Response" doc:name="Transform to a valid response" />
      </response>
   </flow>
   <sub-flow name="Client-Unsecure-Simple-Endpoint">
      <logger message="Begins the unsecure simple client flow" level="INFO"
         doc:name="Logger" />
      <cxf:simple-client serviceClass="${endpoint.simple.class}"
         doc:name="Simple client" operation="${wsdl.operation}" />
      <outbound-endpoint doc:name="Unsecure simple endpoint"
         ref="Endpoint-Unsecure-Simple" />
      <logger message="Ends the unsecure simple client flow" level="INFO"
         doc:name="Logger" />
   </sub-flow>

   <flow name="Client-Unsecure-Proxy-HTTP-Listener">
      <http:listener config-ref="Listener-Application" path="${client.unsecure.proxy.path}"
         doc:name="HTTP Inbound Endpoint" />
      <flow-ref name="Read-HTTP-Variables" doc:name="Read HTPP variables" />
      <flow-ref name="Client-Unsecure-Proxy-Endpoint" doc:name="Send request" />
      <response>
         <flow-ref name="Transform-Response" doc:name="Transform to a valid response" />
      </response>
   </flow>
   <sub-flow name="Client-Unsecure-Proxy-Endpoint">
      <logger message="Begins the unsecure proxy client flow" level="INFO"
         doc:name="Logger" />
      <cxf:jaxws-client operation="${wsdl.operation}"
         serviceClass="${endpoint.codeFirst.interface}" doc:name="Code-First client" />
      <outbound-endpoint ref="Endpoint-Unsecure-Proxy"
         doc:name="Unsecure proxy endpoint" />
      <logger message="Ends the unsecure proxy client flow" level="INFO"
         doc:name="Logger" />
   </sub-flow>

</mule>
